    "math": {
      "basic": {
        "description": "Basic mathematical operations",
        "functions": [
          "abs",
          "min",
          "max",
          "pow",
          "sqrt",
          "exp",
          "log"
        ]
      },
      "trigonometry": {
        "description": "Trigonometric functions",
