{
  "name": "fax-stdlib",
  "version": "0.1.0",
  "description": "Standard library for Fax programming language",
  "modules": {
    "core": {
      "types": {
        "description": "Basic type definitions",
        "functions": [
          "typeof",
          "size_of",
          "align_of"
        ]
      },
      "values": {
        "description": "Value manipulation functions",
        "functions": [
          "clone",
          "move",
          "swap"
        ]
      },
      "operators": {
        "description": "Operator implementations",
        "functions": [
          "add",
          "subtract",
          "multiply",
          "divide",
          "modulo",
          "equal",
          "not_equal",
          "less_than",
          "greater_than",
          "less_equal",
          "greater_equal"
        ]
      }
    },
    "io": {
      "console": {
        "description": "Console input/output operations",
        "functions": [
          "print",
          "println",
          "input",
          "read_line"
        ]
      },
      "file": {
        "description": "File I/O operations",
        "functions": [
          "open_file",
          "read_file",
          "write_file",
          "close_file"
        ]
      }
    },
    "mem": {
      "allocator": {
        "description": "Memory allocation functions",
        "functions": [
          "allocate",
          "deallocate",
          "reallocate"
        ]
      },
      "smart_pointers": {
