struct ListNode {
    val: int,
    next: ptr<ListNode>
}

fn test_linked_list() {
    let node1: ListNode;
    let node2: ListNode;
    
    node1.val = 1;
    node2.val = 2;
    
    node1.next = &node2;
    
    println("Node 1 value:", node1.val);
    println("Node 2 value via Node 1:", (*(node1.next)).val);
}

fn main() {
    test_linked_list();
}

main();
