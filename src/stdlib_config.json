        "functions": [
          "create_map",
          "insert",
          "get",
          "remove",
          "has_key"
        ]
      },
      "set": {
        "description": "Set operations",
        "functions": [
          "create_set",
          "add",
          "remove",
          "has_element",
          "union",
          "intersection"
        ]
      }
